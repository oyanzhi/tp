<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <HBox spacing="8" AnchorPane.leftAnchor="8.0" AnchorPane.rightAnchor="8.0"
              AnchorPane.topAnchor="4.0" AnchorPane.bottomAnchor="4.0">
            <Label fx:id="indexLabel"   styleClass="cell_small_label" minWidth="20.0" wrapText="false"/>
            <Label fx:id="contentLabel" styleClass="cell_small_label" wrapText="true"/>
        </HBox>
    </children>
</AnchorPane>
